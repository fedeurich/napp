<%- include('../partials/head') %>
<link rel="stylesheet" href="/css/login.css" />
<title>Mundo 3D</title>
</head>

<body>
  <%- include('../partials/header') %>

  <main class="containerMainLogin">
    <div class="containerRegistroLogin">
      <form class="registro">
        <label for="email">Correo Electrónico: </label>
        <input type="email" class="control" id="email" name="email" required />
        <label for="contrasena">Contraseña: </label>
        <input type="password" class="control" id="contrasena" name="contrasena" required />
        <button type="submit" class="subir-sesion">Iniciar Sesión</button>
      </form>
    </div>
  </main>

  <%- include('../partials/footer') %>
</body>

</html>